<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_coffee"
        android:id="@+id/coffee"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bar"
        app:layout_constraintLeft_toLeftOf="@+id/coffee"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/coffee"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/coffee"
        app:layout_constraintHorizontal_bias="0.666" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_wifi"
        app:layout_constraintLeft_toRightOf="@+id/coffee"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:id="@+id/floatingActionButton2" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Wifi"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton2"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/floatingActionButton2"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/floatingActionButton2" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_wc"
        app:layout_constraintLeft_toRightOf="@+id/floatingActionButton3"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:id="@+id/floatingActionButton4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="AuditÃ³rio"
        app:layout_constraintLeft_toLeftOf="@+id/floatingActionButton3"
        app:layout_constraintRight_toRightOf="@+id/floatingActionButton3"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton3" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_auditorio"
        app:layout_constraintLeft_toRightOf="@+id/floatingActionButton2"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:id="@+id/floatingActionButton3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        android:text="WC"
        app:layout_constraintLeft_toLeftOf="@+id/floatingActionButton4"
        app:layout_constraintRight_toRightOf="@+id/floatingActionButton4"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButton4" />

</android.support.constraint.ConstraintLayout>
